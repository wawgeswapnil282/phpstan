{
    "name": "githubactions",
    "description": "This is test repo to try github actions",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10.50"
    },
    "autoload": {
        "psr-4": {
            "YourNamespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YourNamespace\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
